<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:background="#3A2A5A"
        android:layout_width="match_parent"
        android:layout_height="35dp">


    <TextView
            android:id="@+id/tv_open_price_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#ffffff"
            android:text="价格"
            android:layout_marginLeft="6dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/tv_close_price_title"
    />

    <TextView
            android:id="@+id/tv_open_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#D9001B"
            android:text="90.480"
            android:layout_marginLeft="6dp"
            app:layout_constraintLeft_toRightOf="@id/tv_open_price_title"
            app:layout_constraintTop_toTopOf="@id/tv_open_price_title"
    />

    <TextView
            android:id="@+id/tv_close_price_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#ffffff"
            android:text="价格"
            app:layout_constraintLeft_toLeftOf="@id/tv_open_price_title"
            app:layout_constraintTop_toBottomOf="@id/tv_open_price_title"
            app:layout_constraintBottom_toBottomOf="parent"
    />

    <TextView
            android:id="@+id/tv_close_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#D9001B"
            android:text="90.480"
            app:layout_constraintLeft_toLeftOf="@id/tv_open_price"
            app:layout_constraintTop_toTopOf="@id/tv_close_price_title"
    />

    <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guide_line1"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.24"
    />


    <TextView
            android:id="@+id/tv_high_price_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#ffffff"
            android:text="最高"
            app:layout_constraintLeft_toRightOf="@id/guide_line1"
            app:layout_constraintTop_toTopOf="@id/tv_open_price_title"
    />

    <TextView
            android:id="@+id/tv_low_price_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#ffffff"
            android:text="最低"
            app:layout_constraintLeft_toLeftOf="@id/tv_high_price_title"
            app:layout_constraintTop_toTopOf="@id/tv_close_price_title"
    />

    <TextView
            android:id="@+id/tv_high_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#D9001B"
            android:text="208.445"
            android:layout_marginLeft="6dp"
            app:layout_constraintTop_toTopOf="@id/tv_high_price_title"
            app:layout_constraintLeft_toRightOf="@+id/tv_high_price_title"
    />

    <TextView
            android:id="@+id/tv_low_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#D9001B"
            android:text="208.335"
            app:layout_constraintTop_toTopOf="@id/tv_low_price_title"
            app:layout_constraintLeft_toLeftOf="@+id/tv_high_price"
    />

    <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guide_line2"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.466"
    />

    <TextView
            android:id="@+id/tv_diff_rate_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#ffffff"
            android:text="涨跌幅"
            app:layout_constraintLeft_toRightOf="@id/guide_line2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/tv_diff_price_title"
    />

    <TextView
            android:id="@+id/tv_diff_price_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#ffffff"
            android:text="涨跌额"
            app:layout_constraintLeft_toLeftOf="@id/tv_diff_rate_title"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_diff_rate_title"
    />

    <TextView
            android:id="@+id/tv_diff_rate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#D9001B"
            android:text="+0.31%"
            android:layout_marginLeft="6dp"
            app:layout_constraintTop_toTopOf="@id/tv_diff_rate_title"
            app:layout_constraintLeft_toRightOf="@+id/tv_diff_rate_title"
    />

    <TextView
            android:id="@+id/tv_diff_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#D9001B"
            android:text="+0.380"
            app:layout_constraintTop_toTopOf="@id/tv_diff_price_title"
            app:layout_constraintLeft_toLeftOf="@+id/tv_diff_rate"
    />

    <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guide_line3"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.714"
    />

    <TextView
            android:id="@+id/tv_volume_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#ffffff"
            android:text="成交量"
            app:layout_constraintLeft_toRightOf="@id/guide_line3"
            app:layout_constraintTop_toTopOf="@id/tv_diff_rate_title"
    />

    <TextView
            android:id="@+id/tv_total_price_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#ffffff"
            android:text="成交额"
            app:layout_constraintLeft_toLeftOf="@id/tv_volume_title"
            app:layout_constraintTop_toTopOf="@id/tv_diff_price_title"
    />

    <TextView
            android:id="@+id/tv_volume"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#C0CCE0"
            android:text="2137.26万股"
            android:layout_marginLeft="6dp"
            app:layout_constraintTop_toTopOf="@id/tv_volume_title"
            app:layout_constraintLeft_toRightOf="@+id/tv_volume_title"
    />

    <TextView
            android:id="@+id/tv_total_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:textColor="#D9001B"
            android:text="2456.75万"
            app:layout_constraintTop_toTopOf="@id/tv_total_price_title"
            app:layout_constraintLeft_toLeftOf="@+id/tv_volume"
    />


</androidx.constraintlayout.widget.ConstraintLayout>