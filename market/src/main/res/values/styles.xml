<resources>

    <style name="TabLayoutTextStyle" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="MockStockFundAccountTitle">
        <item name="android:textColor">#A1A1A1</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="MockStockFundAccountContent">
        <item name="android:textColor">#282828</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="SimulationTradingTitleBar">
        <item name="zr_right_icon">@mipmap/ic_message</item>
        <item name="zr_right_icon_width">19dp</item>
        <item name="zr_right_icon_hight">20dp</item>
        <item name="zr_right_icon_margin">13dp</item>
        <item name="right2_icon">@mipmap/ic_serach</item>
        <item name="right2_icon_width">20dp</item>
        <item name="right2_icon_height">19dp</item>
        <item name="right2_icon_margin">51dp</item>
    </style>

    <style name="TextView_Table_Right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:drawablePadding">4.5dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:background">@color/color_211F2A</item>
        <item name="android:textColor">@color/color_B3BCD0</item>
    </style>

    <style name="Linelayout_Tab_Right_Content">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@color/color_211F2A</item>
    </style>

    <style name="TextView_Table_Right_Item">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">51dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:drawablePadding">4.5dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:background">@color/color_211F2A</item>
    </style>

    <style name="AllHkStockMoreIconBar">
        <item name="zr_right_icon">@mipmap/icon_fresh</item>
        <item name="zr_right_icon_width">19dp</item>
        <item name="zr_right_icon_hight">20dp</item>
        <item name="zr_right_icon_margin">13dp</item>
        <item name="right2_icon">@mipmap/ic_serach</item>
        <item name="right2_icon_width">20dp</item>
        <item name="right2_icon_height">19dp</item>
        <item name="right2_icon_margin">51dp</item>
    </style>

    <style name="MadrketDetailLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#191821</item>
    </style>
    <style name="StockNumPopText">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">10sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginLeft">7dp</item>
        <item name="android:layout_marginRight">7dp</item>
        <item name="android:background">@drawable/rectangle_333333_stroke_radius</item>
    </style>
    <style name="MainTradeYearsInfoText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_FFFFFFFF</item>
        <item name="android:textSize">10sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginTop">9.5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>
</resources>
