<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StockTransDataView">
        <attr name="title_text" format="string|reference"/>
        <attr name="title_background" format="color"/>
        <attr name="price_text_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="ZRThreePartLineLayout">
        <attr name="zr_line_width" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MarketPartInfoView">
        <attr name="market_info_type" format="integer"/>
    </declare-styleable>

    <declare-styleable name="GlobalStockTipsView">
        <attr name="global_info_type" format="integer"/>
    </declare-styleable>

    <declare-styleable name="MarketFinancinalProfitView">
        <attr name="fina_type" format="integer"/>
    </declare-styleable>

    <declare-styleable name="MarketPieChatView">
        <attr name="piechat_type" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BuyingSellingFilesView">
        <attr name="zr_LayoutManager" format="enum">
            <enum name="linear" value="0"></enum>
            <enum name="grid" value="1"></enum>
        </attr>
        <attr name="zr_paddingLeft" format="dimension|reference"/>
        <attr name="zr_paddingRight" format="dimension|reference"/>
    </declare-styleable>
</resources>